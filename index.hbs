{{!< default}}

<div class="gh-outer">
<main class="site-main gh-inner">

  <div class="post-feed gh-feed">
    {{#get "posts" filter="featured:true" include="tags" as |featured|}}
      {{#foreach featured}}
        {{> "loop"}}
      {{/foreach}}
    {{/get}}
    {{#get "posts" filter="featured:false" include="tags" as |posts|}}
      {{#foreach posts}}
        {{> "loop"}}
      {{/foreach}}
    {{/get}}
  </div>

</main>
</div>